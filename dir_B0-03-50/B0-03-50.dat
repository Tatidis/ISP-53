			TERM
		END
	#enddo

! Definition of the power of the half rods
	(ii=0)
	#do
		(ii=ii+1)
		#if (ii > SIZE(ihrhalf)) #exit
		STRU NT_HEAT
			UNIT 'W'
			PNOM 1.	
			SR1 FACT 1. TERM
			SR1 POWE
				0.		0.W
				(tstep2)	0.W				! There are 6 half intact (or ballooned) heated rods in the diagonal
				(tstep2+10.)    (pmax/2.)
				(tstep4+60.s)   (pmax/2.)
				(tstep4+61.s)   0.W
			TERM
			SC1 MACR (WORD('IHRL2-'//CHAR(ihrhalf[ii]))) TERM
			SR1 PROF
				0.000	0.48
				0.199	0.48
				0.200	0.62
				0.459	0.62
				0.460	0.87
				0.719	0.87
				0.720	1.11
				0.979	1.11
				0.980	1.34
				1.239	1.34
				1.240	1.45
				1.314	1.45
				1.315	1.52
				1.349	1.52
				1.350	2.20
				1.649	2.20
				1.650	1.37
				1.684	1.37
				1.685	1.25
				1.759	1.25
				1.760	1.11
				2.019	1.11
				2.020	0.89
				2.279	0.89
				2.280	0.66
				2.539	0.66
				2.540	0.50
				2.799	0.50
				2.800	0.45
				3.000	0.45
			TERM
		END
	#enddo

! Definition of the power of the internal housing
	STRU NT_HEAT
		UNIT 'W'
		PNOM 1.
		SR1 FACT 1. TERM
		SR1 POWE
			0.s		4000.W
			(tstep1)	4000.W
			(tstep1+10.)	(phouse)
			(tend)		(phouse)

		TERM
		SC1 MACR 'HOUSINGI' TERM
		SR1 PROF
			0.000	1.000
			3.000	1.000
		TERM
	END

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!		END POWER
