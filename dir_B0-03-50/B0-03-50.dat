! Input BC
STRU CONNECTI
	NAME 'IN_FLOW'
	TYPE 'BCIN'
	FROM 'USER' TO 'VESSEL'
	STAT 'ON'

	rel_elev 0.
	SR1 q_m_gas
	        (tini) 		(qgas)	
	        (tstep1) 	(qgas)
	        (tstep1+1.0)	0.
	        (tend)  	0.
	TERM
	SR1 q_m_liq
	        (tini)		0.
		(tstep3-1.)	0.
	        (tstep3)	(qliq)				! According to the ISP-53 Specs, there is a spike far larger than qliq for a few seconds when the water injection commences.
	        (tend)		(qliq)				! TODO. model this behaviour to see if this phenomenon is necessary to describe the system.
	TERM
	P	(pgas0)
	T_liq	(tliq0)
	T_gas	(tgas0)

	(ii=0)
	#do
		(ii=ii+1)
		#if (ii > nchannels) #exit
		CHANNEL (ii)
	#enddo

END


